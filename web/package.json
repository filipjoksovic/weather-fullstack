    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.2.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
